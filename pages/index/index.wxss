/**index.wxss**/
page{
  width: 100%;
  height: 100%;
}
.container{
  width: 100%;
  height: 100%;
}
.overlay-bangding{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: #fff;
  z-index: 99;
}
.overlay{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.8);
  z-index: 100;
}
.overlay-content{
  margin: 200rpx auto 0;
  width: 610rpx;
  height: 660rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  border-radius: 3px;
  background-color: #fff;
}
.overlay-content .icon-wrap{
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
  margin-top: 50rpx;
  text-align: center;
  background-color: #f1f2f4;
}
.overlay-content .icon-guzhangbaoxiu{
  font-size: 130rpx;
  color: #f38f10;
  line-height: 200rpx;
}
.overlay-content .word{
  font-size: 40rpx;
  margin-top: 50rpx;
  margin-bottom: 100rpx;
}
.overlay-content button{
  width: 80%;
}


swiper{
  width: 750rpx;
  height: 340rpx;
}
swiper image{
  width: 750rpx;
  height: 340rpx;
}
.common-title{
  padding: 20rpx;
  font-size: 36rpx;
  font-weight: 500;
  color: #333;
}
/* .common-content{
  padding: 0 12rpx 20rpx;
} */
.common-item{
  margin: 8rpx 0 20rpx;
  height: 120rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-around;
}
.common-item .iconfont{
  font-size: 54rpx;
}
.common-item .name{
  font-size: 26rpx;
}